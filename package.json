{
    "name": "lsdyna-ls-prepost-cfile-support",
    "displayName": "LSDYNA LS-PrePost CFile Support",
    "publisher": "DavidGable",
    "description": "This is an unofficial language support for the cfile format used in LSDYNA LS-PrePost.",
    "version": "1.0.2",
    "engines": {
        "vscode": "^1.108.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lsppcfile",
                "aliases": [
                    "LS-PrePost CFile",
                    "lsppcfile"
                ],
                "extensions": [
                    ".cfile"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "light": "./icons/lsppcfile-light-dark.png",
                    "dark": "./icons/lsppcfile-light-dark.png"
                }
            }
        ],
        "grammars": [
            {
                "language": "lsppcfile",
                "scopeName": "source.lsprepost-cfile",
                "path": "./syntaxes/lsppcfile.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "lsppcfile",
                "path": "./snippets.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Hydrowelder/lsdyna-ls-prepost-cfile-support.git"
    },
    "keywords": [
        "snippets",
        "snippet",
        "language-support",
        "lsdyna",
        "semantic-highlighting",
        "semantic-highlighter",
        "LS-PrePost",
        "lspp",
        "cfile",
        "finite-element",
        "FEA",
        "simulation",
        "postprocessing",
        "visualization",
        "engineering",
        "modeling"
    ],
    "icon": "icons/lsppcfile-light-dark.png"
}
